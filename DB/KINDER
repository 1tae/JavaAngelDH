CREATE TABLE MEM(
    LOGIN_ID VARCHAR2(15) PRIMARY KEY,
    LOGIN_PWD VARCHAR2(30) NOT NULL,
    MEM_NAME VARCHAR2(12) NOT NULL,
    MEM_GENDER CHAR CHECK(MEM_GENDER IN('M','F')),
    EMAIL VARCHAR2(30),
    PHONE VARCHAR2(30) NOT NULL,
    ADDRESS VARCHAR2(50) NOT NULL,
    STU_NAME VARCHAR2(15) NOT NULL,
    STU_BIRTH DATE,
    STU_GENDER CHAR CHECK(STU_GENDER IN('M','F')),
    CLASS_NAME VARCHAR(15)
);

